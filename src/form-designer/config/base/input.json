    },
    {
      "type": "Switch",
      "name": "allowClear",
      "valuePropName": "checked",
      "label": "是否允许清除"
    },
    {
      "type": "Input",
      "name": "placeholder",
      "label": "提示文案"
    },
    {
      "type": "InputNumber",
      "name": "maxLength",
      "label": "最大长度"
    },
    {
      "type": "BindFunction",
      "name": "onChange",
      "label": "设置onChange"
